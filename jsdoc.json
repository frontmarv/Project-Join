{
  "source": {
    "include": ["scripts"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/minami"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}